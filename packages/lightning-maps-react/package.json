{
  "name": "lightning-maps-react",
  "version": "0.0.23",
  "main": "dist/build.js",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rimraf dist",
    "clean:dev": "rimraf examples/dist",
    "dev": "npm run clean:dev && webpack-dev-server --config examples/webpack.config.js --mode development",
    "test": "npm run standard",
    "standard": "standard \"**/*.js\"",
    "bump": "npx standard-version"
  },
  "dependencies": {
    "lightning-maps": "file:../lightning-maps",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
